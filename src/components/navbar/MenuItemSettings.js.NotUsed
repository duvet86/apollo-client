const MenuItemSettings = ({ appKey }) => (
  <LinkContainer to={`/${appKey}/settings`}>
    <MenuItem>
      <i className="fa fa-cog" aria-hidden="true" />{" "}Settings
    </MenuItem>
  </LinkContainer>
);

MenuItemSettings.propTypes = {
  appKey: PropTypes.string.isRequired
};

export default MenuItemSettings;
